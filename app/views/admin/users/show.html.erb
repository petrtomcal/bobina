<h1>User detail</h1>
<table class="show-table" cellpadding="0" cellspacing="0">
  <tr>
    <th>Email</th>
    <td><%= @user.email %></td>
  </tr>
  <tr>
    <th>Name</th>
    <td><%= @user.first_name %></td>
