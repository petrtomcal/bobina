<h1>Paypal setting detail</h1>
<table class="show-table" cellpadding="0" cellspacing="0">
  <tr>
    <th>Email</th>
    <td><%= @setting.email %></td>
  </tr>
  <tr>
    <th>Secret</th>
    <td><%= @setting.secret %></td>
  </tr>
  <tr>
    <th>Certificate ID</th>
    <td><%= @setting.cert_id%></td>
  </tr>
  <tr>
    <th>PayPal URL</th>
    <td><%= @setting.url%></td>
  </tr>
</table>
<br/>

<%= link_to 'Edit', :action => 'edit',:id => @setting.user_id %> |
<%= link_to 'Back', :controller => 'users', :action => 'edit', :id => @setting.user_id  %>



