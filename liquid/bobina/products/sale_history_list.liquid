<div class="sub-list">
<table class="common-table" cellpadding="0" cellspacing="0">
  <thead>
   <tr>    
      <th colspan="5">&nbsp;</th>
   </tr>
  </thead>
{% for sale in sales %}
{% if sale.user_id != nil %}
  <tr>
    <td>{{ sale.id }}</td>
    <td>{{ sale.created_at }}</td>
    <td>{{ sale.purchased_at }}</td>
    {% if sale.paid == 1 %}
      <td>{{ sale.token }}</td>
    {% endif %}  
    <td>
    link to details
    </td>    
  </tr>
{% endif %}  
{% endfor %}    
</table>

<table class="common-table" cellpadding="0" cellspacing="0">
  <thead>
   <tr>    
      <th colspan="5">&nbsp;</th>
   </tr>
  </thead>
{% for product in products %}
  <tr>
    <td>{{ product.id }}</td>
    <td>{{ product.name | truncate: 20 }}</td>
    <td>{{ product.price|number_to_usd }}</td>
    <td>{{ product.category_id }}</td>
    <td>
    <a href="/products/show/{{product.id}}">show</a>  
    <a href="/products/download_links/{{product.id}}">download links</a>
    </td>    
  </tr>
{% endfor %}    
</table>

<table class="common-table" cellpadding="0" cellspacing="0">
  <thead>
   <tr>    
      <th colspan="5">&nbsp;</th>
   </tr>
  </thead>
{% for pack in packs %}
  <tr>
    <td>{{ pack.id }}</td>
    <td>{{ pack.name | truncate: 20 }}</td>
    <td>{{ pack.price|number_to_usd }}</td>
    <td>{{ pack.category_id }}</td>
    <td>
    <a href="/packs/show/{{pack.id}}">show</a>
    </td>    
  </tr>
{% endfor %}    
</table>

